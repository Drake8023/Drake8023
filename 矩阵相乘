#include <stdio.h>
void main()
{
	int i, j, k;
	int a[3][3], b[3][3], m[3][3];
	for (i = 0; i < 3; i++)
	{
		for (j = 0; j < 3; j++)
		{
			m[i][j] = 0;
		}
	}
	a[0][0] = 5, a[0][1] = 9, a[0][2] = 4;
	a[1][0] = 5, a[1][1] = 7, a[1][2] = 11;
	a[2][0] = 12, a[2][1] = 6, a[2][2] = 2;

	b[0][0] = 4, b[0][1] = 12, b[0][2] = 22;
	b[1][0] = 7, b[1][1] = 14, b[1][2] = 2;
	b[2][0] = 9, b[2][1] = 8, b[2][2] = 3;

	for (i = 0; i < 3; i++)
	{
		for (j = 0; j < 3; j++)
		{
			for (k = 0; k < 3; k++)
			{
				m[i][j] = m[i][j] + a[i][k] * b[k][j];
			}
		}
	}
	for (i = 0; i < 3; i++)
	{
		for (j = 0; j < 3; j++)
		{
			printf("%d", m[i][j]);
			printf(" ");
		}
		printf("\n");
	}
}
