#include<stdio.h>
void main()
{
	int d, z, x,n=0;
	for (d = 0; d < 33;d++)
	{
		for (z = 0; z < 50; z++)
		{
			for (x =0; x < 100; x++)
			{
				if (x % 2 == 0)
					if (3 * d + 2 * z + x / 2 == 100&&d+z+x==100)
					{
						n++;
						printf("第%d种情况\n", n);
						printf("大马%d匹,中马%d匹,小马%d匹\n", d, z, x);
					}
			}
		}
	}
}
